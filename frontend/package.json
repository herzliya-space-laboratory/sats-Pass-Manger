{
  "name": "sats-pass-manger",
  "description": "this app mange the hoope/tavel/tau sats pass and the data collected on them over time",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run watch:tailwind & sapper dev",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "run:cy": "cypress run",
    "open:cy": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@tailwindcss/ui": "^0.6.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "flatpickr": "^4.6.6",
    "jsonwebtoken": "^9.0.0",
    "session-file-store": "^1.5.0",
    "sirv": "^0.4.0",
    "svelte-forms-lib": "^1.2.2"
  },
  "devDependencies": {
    "cypress": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.1.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "tailwindcss": "^1.8.12",
    "typescript": "^4.1.2",
    "webpack": "^4.7.0"
  }
}
